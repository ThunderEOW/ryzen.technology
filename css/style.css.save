 body {
        font-family: Courier New,Courier,Lucida Sans Typewriter,Lucida Typewriter,monospace;
        background: #2E3436;
} 
/* inherited */ 
 .MainMenu, p {
	display: flex;
}

 .MainMenu p, i, a {
    color: #F95700FF;
	float: left;
	font-size: 24px;
	font-weight: bolder;
}

 .MainMenu i, a {
    color: #F9D300;
	margin-left:10%;
	text-decoration: none;
	transform:scale(1);
	transition:0.5s linear
}

 .MainBody p, sup, pre {
	color: white;
}
/* non-inherited */ 
 .MainMenu a:hover {
	color: #F95700FF;
	transform:scale(1.5);
 	transition:0.25s linear /* jag ar en professionel editor mannen */
}
 .MainBorder {
	position: relative;
	top: 0;
	border-bottom: 5px solid #00A4CCFF;
}
 .MainBody {
	position: relative;
	left: 20px;
}
 
 display:none;
}

.Mainbody p:first-letter {
	margin-left:5.12em;
}

 .Mainbody sup {
	font-size: 75%;
}


 #bigflex {
	color: red;
}

 #DomainName {
	margin-left:0.5%;
}

